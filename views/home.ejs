<%- include("partials/header") -%>

<div class="box" id="heading">
  <h1> <%= Today%></h1>
</div>

<h1>Home</h1>
<p><%= startingContent%></p>

<% posts.forEach(function(post){ %>
  <h1><%= post.title %></h1>
  <p>
    <%= post.content.substring(0,100) + "..."%>  <a href="/posts/<%=post.title %>">Read More</a>
  </p>

<% }) %>

<a href="/compose"><h3>Add a New Post!</h3></a>

